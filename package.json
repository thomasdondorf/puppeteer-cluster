{
  "name": "puppeteer-cluster",
  "version": "0.2.0",
  "description": "Cluster for puppeteer",
  "main": "lib/main.js",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "dev": "./node_modules/.bin/tsc --watch",
    "lint": "./node_modules/.bin/tslint -c tslint.json --project tsconfig.json 'src/**/*.ts'"
  },
  "author": "",
  "dependencies": {
    "debug": "^3.1.0",
    "puppeteer": "^1.5.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.3",
    "@types/puppeteer": "^1.3.4",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts}"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
